{
  "name": "scout-eda",
  "version": "0.1.0",
  "scripts": {
    "build_notworking": "rm -rf dist && tsc -p app",
    "build": "tsc -p app",
    "start": "http-server -c-1 -o -p 8875 .",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    
    "start_old": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "",
  "dependencies": {
    "a2-in-memory-web-api": "^0.1.9",
    "angular2": "2.0.0-beta.9",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp-cli": "git://github.com/gulpjs/gulp-cli.git#4.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.1.0",
    "remap-istanbul": "^0.5.1",
    "systemjs": "^0.19.24",
    "typescript": "^1.8.9",
    "typings": "^0.7.5"
  }
}
